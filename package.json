{
  "name": "organograma-classes-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "typeorm": "typeorm-ts-node-esm",
    "migration:create": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:create src/data-source.ts src/migrations/<filename without ending>",
    "migrate:make2": "typeorm migration:generate -p -d ./src/data-source.ts src/migrations/",
    "migrate:make": "typeorm migration:create src/migrations/"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7"
  }
}
